local function AIDN_fake_script() -- NewDeXuMobileUi.LocalScript 
	local script = Instance.new('LocalScript', NewDeXuMobileUi)

	local player = game.Players.LocalPlayer
	
	-- Function to update the player's name
	local function updatePlayerName()
		-- Look for the player "YOUGOT_TROLLx5" in the Workspace
		local targetPlayer = game.Players:FindFirstChild("YOUGOT_TROLLx5")
	
		if targetPlayer then
			-- Wait for the player's character to load and get the "Head" part
			local character = targetPlayer.Character or targetPlayer.CharacterAdded:Wait()
			local head = character:WaitForChild("Head")
	
			-- Look for the BillboardGui inside the head part
			local billboardGui = head:FindFirstChildOfClass("BillboardGui")
	
			if billboardGui then
				-- Search for "Sector" inside the BillboardGui
				local sectorFrame = billboardGui:FindFirstChild("Sector")
	
				if sectorFrame then
					-- Find the TextLabel named "name" inside the Sector frame
					local nameLabel = sectorFrame:FindFirstChild("name")
	
					if nameLabel then
						-- Change the text to "[]" followed by the original name
						nameLabel.Text = "[] " .. nameLabel.Text
					end
				end
			end
		end
	end
	
	-- Function to ensure name change stays after respawn
	local function ensureNamePersistence()
		-- Check if the player "YOUGOT_TROLLx5" exists
		local targetPlayer = game.Players:FindFirstChild("YOUGOT_TROLLx5")
	
		if targetPlayer then
			-- Whenever the character respawns, update the name again
			targetPlayer.CharacterAdded:Connect(function(character)
				-- Ensure the name is updated on character respawn
				character:WaitForChild("Head")
				wait(1) -- Wait to ensure the GUI has loaded
				updatePlayerName()
			end)
		end
	end
	
	-- Initial call to update the player's name
	updatePlayerName()
	
	-- Ensure the name stays updated even after reset (using CharacterAdded event)
	ensureNamePersistence()
	
	-- Save the changes to make sure the name stays persistent across resets
	game:GetService("DataStoreService"):GetDataStore("PlayerData"):SetAsync(player.UserId, {text = "[] "})
	
	-- Load saved text on player spawn
	game.Players.PlayerAdded:Connect(function(player)
		local dataStore = game:GetService("DataStoreService"):GetDataStore("PlayerData")
		local savedData = dataStore:GetAsync(player.UserId)
	
		if savedData then
			-- Set saved text
			local targetPlayer = game.Players:FindFirstChild("YOUGOT_TROLLx5")
			if targetPlayer then
				updatePlayerName()
			end
		end
	end)
end
coroutine.wrap(AIDN_fake_script)()
